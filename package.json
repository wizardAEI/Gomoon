{
  "name": "gomoon",
  "productName": "Gomoon",
  "version": "1.1.3",
  "description": "A ai tools for everyone on Mac, Windows and Linux.",
  "main": "./out/main/index.js",
  "author": "aei",
  "license": "Apache-2.0",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "electron-vite build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@langchain/community": "^0.0.52",
    "@langchain/core": "^0.1.60",
    "@langchain/google-genai": "^0.0.12",
    "@langchain/openai": "^0.0.34",
    "@mozilla/readability": "^0.5.0",
    "@solidjs/router": "^0.13.2",
    "@types/markdown-it": "^14.0.1",
    "@types/prismjs": "^1.26.3",
    "@types/ws": "^8.5.10",
    "@vscode/markdown-it-katex": "^1.0.3",
    "@xenova/transformers": "^2.17.1",
    "@zag-js/checkbox": "^0.50.0",
    "@zag-js/popper": "^0.50.0",
    "@zag-js/radio-group": "^0.50.0",
    "@zag-js/solid": "^0.50.0",
    "@zag-js/switch": "^0.50.0",
    "@zag-js/tooltip": "^0.50.0",
    "apache-arrow": "^16.0.0",
    "cheerio": "^1.0.0-rc.12",
    "d3-dsv": "2",
    "electron-updater": "^6.1.8",
    "highlight.js": "^11.9.0",
    "jsdom": "^24.0.0",
    "langchain": "^0.1.36",
    "libreoffice-convert": "^1.5.1",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "mammoth": "^1.7.1",
    "markdown-it": "^14.1.0",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-highlightjs": "^4.0.1",
    "moment": "^2.30.1",
    "node-fetch": "^2.6.4",
    "node-llama-cpp": "^2.8.9",
    "officeparser": "^4.0.8",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^22.7.1",
    "solid-js": "^1.8.17",
    "solidjs-use": "^2.3.0",
    "tesseract.js": "^5.0.5",
    "ulid": "^2.3.0",
    "vectordb": "^0.4.16",
    "ws": "^8.16.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "autoprefixer": "^10.4.19",
    "electron": "29.3.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.2.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-solid": "^0.13.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "tailwindcss-themer": "^4.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-solid": "^2.10.2"
  },
  "packageManager": "yarn@3.2.0"
}
